   "source": [
    "import apache_beam as beam\n",
    "\n",
    "with beam.Pipeline() as p1:\n",
    "    word_count = (\n",
    "        p1\n",
    "        |beam.io.ReadFromText('data.txt')\n",
    "        |beam.FlatMap(lambda x : [x for x in x.split(\" \")])\n",
    "        |beam.combiners.Count.Globally()\n",
    "        |beam.Map(print)\n",
    "\n",
    "\n",
    "    )"
   ]
